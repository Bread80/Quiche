program
  type TArray = array[0..5] of Byte

  var
    TestOld: Byte
    TestNew: Byte

  procedure FillArray(A: TArray);stack;
  begin
    TestOld := A[5]
    for var I := 0 to 5 do
      A[I] := I
    TestNew := A[5]
  end

  var MyArray: TArray;
  begin
    for var I := 0 to 5 do
      MyArray[I] := $ff
    FillArray(MyArray)
  end.
