te=$99 var b:=lo(a)endcodecompile BytePassedToHiLoSwapcode lo Int8 error var a:Int8=$79 var b:=lo(a)endcodecompile BytePassedToHiLoSwap;-- Low (Lowest value in enumerable type)code low  var a:=low(Integer)  var b:=low(Int8)  var c:=low(Byte)  var d:=low(Word)  var e:=low(Char)  var f:=low(Boolean)    var g:=low(a)  var h:=low(b)  var i:=low(c)  var j:=low(d)  var k:=low(e)  var l:=low(f)    var m:Pointer  var n:=low(m)endcodevarvalue a -32768varvalue b -128varvalue c 0varvalue d 0varvalue e #0varvalue f Falsevartype a Integervartype b Int8vartype c Bytevartype d Wordvartype e Charvartype f Booleanvarvalue g -32768varvalue h -128varvalue i 0varvalue j 0varvalue k #0varvalue l Falsevartype g Integervartype h Int8vartype i Bytevartype j Wordvartype k Charvartype l Booleanvarvalue m 0vartype m pointer;-- Ordcode ord  var a:=ord('a')  var b:='Z'  var c:=ord(b)    var d:Word=ord(65535)  var e:=ord(-32768)  var f:=ord(127)endcodevarvalue a 97varvalue c 90vartype a Bytevartype c Bytevarvalue d 65535vartype d wordvarvalue e -32768vartype e Integervarvalue f 127vartype f Integer;-- Out(put);Don't have a way to autotest this yet. Needs a test fixture in the emulator for I/O portscode out  out($ff00,128)  var p:=$1234  var x:Byte=$fe  out(p,64)  out($4321,x)  out(p,x)endcodecompile noerror;-- Peek;Another one we can't autotest yetcode peek addr const  var a:=peek($1000)  var b:=peek(200)  var c:=peek($1000,Byte)  var d:=peek($1000,Word)  var e:=peek($acbd,Pointer)  var f:=peek(0,Int8)  var g:=peek($ffff,Integer)  var h:=peek(65535,Boolean)  var i:=peek($8000,Char)endcodevartype a Bytevartype b Bytevartype c Bytevartype d wordvartype e Pointervartype f Int8varType g Integervartype h Booleanvartype i Charcode peek variable  var a:=$ffff  var b:=peek(a)  var c:=peek(a, Integer)  var d:=100  var e:=peek(d)  var f:Int8=20  var g:=peek(f)  var h:Integer=2000  var i:=peek(h)endcodevartype b bytevartype c Integervartype e Bytevartype g Bytevartype i Bytecode peek variable typedef  var a:Integer  a:=peek($1000,a)endcodecompile noerror;-- Poke;(And again we can't autotest properly (yet));This basically tests for compilation errorscode poke  var i:Integer=100  var i8:Int8=50  var b:Byte=255  var w:Word=$c000  var w2:Word=$00c0  var p:Pointer=$c002  var p2:Pointer=$00c2    poke($c004,$ff)  poke(w,$ff)  poke(i,$ff)  poke($c006,b)  poke($c008,w2)  poke($c00a,i)  poke(w,b)  poke(w,i)  poke(w2,w2)  poke(p,p2)  poke(p2,p2)    poke(w,False)  poke(w,'Z')endcodecompile noerrorcode poke Out of range  poke($c004,$ffff)endcodecompile ConstantOutOfRangecode poke Out of range 2  var w:Word=$c000  poke($c008,w)endcoderuntime rangecode poke Out of range 3  var p:Pointer=$c002  poke(p,p)endcoderuntime range;--Pred (previous item in enumerated type);TODO: Separate tests for overflow checked and not overflow checkedcode pred  var a:Integer=100  var b:Word=6000  var c:Int8=-20  var d:Integer=-1000  var e:Pointer=1000  var f:Char='z'    a:=pred(a)  b:=pred(b)  c:=pred(c)  d:=pred(d)  e:=pred(e)  f:=pred(f)    var m:=pred(a,10)  var n:=pred(b,10)  var o:=pred(c,10)  var p:=pred(d,10)  var q:=pred(e,10)  var r:=pred(f,10)  endcodevarvalue a 99varvalue b 5999varvalue c -21varvalue d -1001varvalue e 999varvalue f 'y'varvalue m 89varvalue n 5989varvalue o -31varvalue p -1011varvalue q 989varvalue r 'o'$overflow off;Compiler can't overflow check Pred(Boolean).code pred no overflow  var g:=true  g:=pred(g)endcodevarvalue g Falsecode pred constant  var a:=pred(10)  var b:=pred('c')  var c:=pred(true)  var d:=pred(10,2)endcodevarvalue a 9varvalue b 'b'varvalue c falsevarvalue d 8;--Sizeof (size of type, variable, etc)code sizeof vars  var a:Integer=3  var b:Int8=3  var c:Byte=3  var d:Word=3  var e:Pointer=3  var f:Char='a'  