;Basic operators/primitives/intrinsics;==================Typecastscode integer compile time  var a:=integer(16)  var b:=integer(-16)  var c:=integer(16000)  var d:=integer(-16000)  var e:=integer(65535)  var f:=integer('A')  var g:=integer(true)endcodevarvalue a 16varvalue b -16varvalue c 16000varvalue d -16000varvalue e -1varvalue f 65varvalue g 255code integer runtime  var w:word=16  var a:=integer(w)  w := 16000  var c:=integer(w)  w := 65535  var e:=integer(w)  var ch:char='A'  var f:=integer(ch)  var bool:boolean=true  var g:=integer(bool)endcodevarvalue a 16varvalue c 16000varvalue e -1varvalue f 65varvalue g 255  code int8 compile time  var a:int8=int8(16)  var b:int8=int8(-16)  var c:int8=int8(257)  var d:int8=int8(65535)  var f:int8=int8('A')endcodevarvalue a 16varvalue b -16varvalue c 1varvalue d -1varvalue f 65code int8 runtime  var w:word=16  var a:int8=int8(w)  w := 257  var c:int8=int8(w)  w := 65535  var e:int8=int8(w)  var ch:char='A'  var f:int8=int8(ch)  var by:byte=25  var g:int8=int8(by)  by:=255  var h:int8=int8(by)  var i16:integer=100  var j:int8=int8(i16)  i16:=257  var k:int8=int8(i16)endcodevarvalue a 16varvalue c 1varvalue e -1varvalue f 65varvalue g 25varvalue h -1varvalue j 100varvalue k 1code word compile time  var a:word=word(16)  var b:word=word(-1)  var c:word=word(32767)  var d:word=word(-32768)  var f:word=word('A')endcodevarvalue a 16varvalue b 65535varvalue c 32767varvalue d 32768varvalue f 65code word runtime  var i:=16  var a:word=word(i)  i := -1  var b:word=word(i)  i := 32767  var c:word=word(i)  i:= -32768  var d:word=word(i)  var ch:char='A'  var f:word=word(ch)  var i8:int8=25  var g:word=word(i8)  i8:=-128  var h:word=word(i8)endcodevarvalue a 16varvalue b 65535varvalue c 32767varvalue d 32768varvalue f 65varvalue g 25varvalue h 128  code byte compile time  var a:byte=byte(16)  var b:byte=byte(-1)  var c:byte=byte(257)  var d:byte=byte(65535)  var f:byte=byte('A')endcodevarvalue a 16varvalue b 255varvalue c 1varvalue d 255varvalue f 65code byte runtime  var i:=16  var a:byte=byte(i)  i := -1  var b:byte=byte(i)  i:=257  var c:byte=byte(i)  i := 32767  var d:byte=byte(i)  var ch:char='A'  var f:byte=byte(ch)endcodevarvalue a 16varvalue b 255varvalue c 1varvalue d 255varvalue f 65  code pointer compile time  var a:pointer=pointer(16)  var b:pointer=pointer(-1)  var c:pointer=pointer(32767)  var d:pointer=pointer(-32768)  var f:pointer=pointer('A')endcodevarvalue a 16varvalue b 65535varvalue c 32767varvalue d 32768varvalue f 65code pointer runtime  var i:=16  var a:pointer=pointer(i)  i := -1  var b:pointer=pointer(i)  i := 32767  var c:pointer=pointer(i)  i:= -32768  var d:pointer=pointer(i)  var ch:char='A'  var f:pointer=pointer(ch)  var i8:int8=25  var g:pointer=pointer(i8)  i8:=-128  var h:pointer=pointer(i8)endcodevarvalue a 16varvalue b 65535varvalue c 32767varvalue d 32768varvalue f 65varvalue g 25varvalue h 128;Booleancode boolean compile time  var a:boolean=boolean(false)  var b:boolean=boolean(true)endcodevarvalue a falsevarvalue b truecode boolean runtime  var i:=255  var a:boolean=boolean(i)  i := 0  var b:boolean=boolean(i)  var by:byte=255  var c:boolean=boolean(by)endcodevarvalue a truevarvalue b falsevarvalue c true   ;Charcode char compile time  var a:char=char(33)  var b:char=char(256+65)endcodevarvalue a '!'varvalue b 'A'code char runtime  var i:=33  var a:char=char(i)  i := 256+65  var b:char=char(i)endcodevarvalue a '!'varvalue b 'A'